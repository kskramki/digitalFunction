    <UserSecretsId>2e50f3cf-9713-40a3-af5f-af4f5f574d49</UserSecretsId>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="FubarCoder.RestSharp.Portable.Core" Version="4.0.8" />
